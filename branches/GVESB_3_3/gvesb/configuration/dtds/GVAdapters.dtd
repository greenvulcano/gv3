<!--
    This element defines the drivers relevant to JDBC calls.

    #SortChild
-->
<!ELEMENT Drivers (Description?, Driver*)>
<!--
    This element defines the necessary parameters to define the Driver
     to be used for JDBC connections.

    #Template:
        <xsl:template match="Driver">
            <xsl:if test="Description">
                <small><xsl:value-of select="Description"/></small><p/>
            </xsl:if>
            <table>
                <tr valign="top">
                    <td><b>Enabled:</b></td>
                    <td><xsl:value-of select="@enabled"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Class:</b></td>
                    <td><xsl:value-of select="@class"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Init Type:</b></td>
                    <td><xsl:value-of select="@initType"/></td>
                </tr>
            </table>
        </xsl:template>
-->
<!ELEMENT Driver (Description?)>
<!--
    Unmodifiable.
    Used by GreenVulcanoESB factory framework.
-->
<!ATTLIST Driver type CDATA #FIXED "driver">
<!--
    Defines whether this driver is active or not.
-->
<!ATTLIST Driver enabled (Yes | No) "No">
<!--
    Defines the class driver.
    Ex : oracle.jdbc.driver.OracleDriver

    #NotNull
-->
<!ATTLIST Driver class CDATA #REQUIRED>
<!--
    Some drivers basing on the specifications do not need to be instantiated to be initialized, in this case then
    the initType would be: 'static'. (The Oracle driver is static.)
    Ex : oracle.jdbc.driver.OracleDriver

    #NotNull
-->
<!ATTLIST Driver initType (static | instance) "static">
<!--
    This element defines the necessary parameters to define the
    Driver to be used for JDBC connection.

    #SortChild
-->
<!ELEMENT Connections (Description?, Connection*)>
<!--
    This element defines the necessary parameters to define the
    Driver to be used for JDBC connection.

    #Template:
        <xsl:template match="Connection">
            <xsl:if test="Description">
                <small><xsl:value-of select="Description"/></small><p/>
            </xsl:if>
            <table>
                <tr valign="top">
                    <td><b>ID:</b></td>
                    <td><xsl:value-of select="@id"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Enabled:</b></td>
                    <td><xsl:value-of select="@enabled"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Connection String:</b></td>
                    <td><xsl:value-of select="@connectionString"/></td>
                </tr>
                <tr valign="top">
                    <td><b>User:</b></td>
                    <td><xsl:value-of select="@user"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Password:</b></td>
                    <td><xsl:value-of select="@password"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Auto Commit:</b></td>
                    <td><xsl:value-of select="@autoCommit"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Transaction Isolation:</b></td>
                    <td><xsl:value-of select="@txIsolation"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Connection Min:</b></td>
                    <td><xsl:value-of select="@connectionMin"/></td>
                </tr>
                <tr valign="top">
                    <td><b>Connection Max:</b></td>
                    <td><xsl:value-of select="@connectionMax"/></td>
                </tr>
            </table>
        </xsl:template>
-->
<!ELEMENT Connection (Description?)>
<!--
    Unmodifiable.
    Used by GreenVulcanoESB factory framework.
-->
<!ATTLIST Connection    type CDATA #FIXED "connection">
<!--
    Defines the connection identifier. Used to find the connection in the pool.

    #NotNull
    #Unique: ../Connection/@id
-->
<!ATTLIST Connection	id CDATA #REQUIRED>
<!--
    If true the connection is enabled.
-->
<!ATTLIST Connection	enabled (Yes | No) "No">
<!--
    Defines the connection string to the database.
    Refer to your database documentation for syntax.

    #NotNull
-->
<!ATTLIST Connection    connectionString CDATA #REQUIRED>
<!--
    Defines user access to the database.
    Giving a value to the user is not required if you enter it in the connection String.

    #Warn: {{../Connection[not(@password) and @user]}} If a Username is set a Password MUST be set too
-->
<!ATTLIST Connection    user CDATA #IMPLIED>
<!--
    Defines the password to the database.
    The value of the password is not required if you enter it in the connection string.

    #Warn: {{../Connection[not(@user) and @password]}} If a Password is set a Username MUST be set too
    #Encrypted
-->
<!ATTLIST Connection    password CDATA #IMPLIED>
<!--
    Defines the minimum number of connections to be built at initialization.

    #Pattern: ! [1-9][0-9]* ! Numeric characters only
    #NotNull
-->
<!ATTLIST Connection    connectionMin CDATA #REQUIRED>
<!--
    Defines the maximum number of connections that can be built.

    #Pattern: ! [1-9][0-9]* ! Numeric characters only
    #NotNull
-->
<!ATTLIST Connection    connectionMax CDATA #REQUIRED>
<!--
    Defines how to commit the SQL operations.
    If AutoCommit is 'true' all SQL statements are
    carried out and commit such uniqe transactions.
    If AutoCommit is 'false', but the statement is
    grouped into transactions that end with the call
    to a method to commit or rollback.

    By default, new connections are in autocommit mode.

-->
<!ATTLIST Connection    autoCommit (false | true) "true">
<!--
    Defines the level of transaction isolation of the given connection.

    Just remember the following concepts:

    - 'dirty reads' : a dirty read occurs when a transaction reads data from
                           a row that has been modified by another transaction,
                           but not yet committed.
    - 'non-repeatable' : non-repeatable reads may occur in a lock-based
                                   concurrency control method when read locks are
                                   not acquired when performing a SELECT.
    - 'phantom' : a phantom read occurs when, in the course of a transaction,
                         two identical queries are executed, and the collection of rows
                         returned by the second query is different from the first.

    Therefore we list the meanings of the isolation levels of a
    transaction :

    - TRANSACTION_NONE : Uses DataBase defaults

    - TRANSACTION_READ_UNCOMMITTED : Dirty reads, non-repeatable and phantom reads are possible.

    - TRANSACTION_READ_COMMITTED : Dirty reads not possible, non-repeatable and phantom are possible.

    - TRANSACTION_REPEATABLE_READ : Dirty reads and non-repeatable are not possible, phantom is possible.
    - TRANSACTION_SERIALIZABLE : Dirty reads, non-repeatable and phantom are not possible.

-->
<!ATTLIST Connection    txIsolation (TRANSACTION_NONE | TRANSACTION_READ_UNCOMMITTED | TRANSACTION_READ_COMMITTED | TRANSACTION_REPEATABLE_READ | TRANSACTION_SERIALIZABLE) "TRANSACTION_NONE">

