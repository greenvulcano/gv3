<!--
    List of configured Data Providers.

    #SortChild
    #Template:
        <xsl:template match="DataProviders">
            <xsl:if test="Description">
                <small><xsl:value-of select="Description"/></small><p/>
            </xsl:if>
            <table cellpadding="0" cellspacing="0">
                <tr valign="top">
                    <td>Configured DataProviders:</td>
                    <td width="20"></td>
                    <td>
                        <table cellpadding="0" cellspacing="0">
                            <tr>
                                <td><b><i>Name</i></b></td>
                                <td width="20"></td>
                                <td><b><i>Type</i></b></td>
                                <td width="20"></td>
                                <td><b><i>Class</i></b></td>
                            </tr>
                            <xsl:for-each select="./*[@type='dataProvider'][@name!='']">
                                <tr>
                                    <td><b><xsl:value-of select="@name"/></b></td>
                                    <td width="20"></td>
                                    <td><b><xsl:value-of select="name(.)"/></b></td>
                                    <td width="20"></td>
                                    <td><b><xsl:value-of select="@class"/></b></td>
                                </tr>
                            </xsl:for-each>
                        </table>
                    </td>
                </tr>
            </table>
        </xsl:template>
-->
<!ELEMENT DataProviders (%dataProviders;)*>

<!-- 
    Allows to define the key and direction of the Expression defined above.
-->
<!ELEMENT Field (Expression)>
<!--
    #Unique: ../*[@type='dataProvider']/@name
    #NotNull
-->
<!ATTLIST Field key CDATA #REQUIRED>
<!--
    Unmodifiable.
    Used by GreenVulcanoESB factory framework.
-->
<!ATTLIST Field type CDATA #FIXED "field">
<!--
  Field direction.
  INOUT: The expression is evaluated on the object set
             (usually the current GVBuffer, unless otherwise specified by the attribute 'source-selector')
             the value returned by the expression, is added to the context with the prefix
             'key_' + @ key, and set the data provider to be returned in the output.
  IN   : The expression is evaluated on the object set
          (usually the current GVBuffer, unless otherwise specified by the attribute 'source-selector')
          the value returned by the expression is added to the context with the prefix
          'key_' + @ key.
  OUT  : The expression is evaluated directly on the object that is returned by the DataProvider.

  In all cases, two variables are added to the context:
  # Input: object set, usually the current GVBuffer, which will not be overwritten from attribute 'source-selector';
  # Output: the object that is returned by the DataProvider.
-->
<!ATTLIST Field direction (INOUT|IN|OUT) #IMPLIED>
<!--
    #NotNull
-->
<!ELEMENT Expression (#PCDATA)>
<!--
    #NotNull
-->
<!ATTLIST Expression type (ognl|js|xpath|regex) #REQUIRED>
<!--
    This elements transforms a GVBuffer into an object array.
    The internal Field element must use the key attribute to identify
