<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>GreenVulcano HTTP Adapter</display-name>
	<description>GreenVulcano HTTP Adapter</description>
	<servlet>
		<servlet-name>HttpInboundGateway</servlet-name>
		<description/>
		<servlet-class>it.greenvulcano.gvesb.adapter.http.HttpInboundGateway</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>HttpInboundGateway</servlet-name>
		<url-pattern>/HttpInbound</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HttpInboundGateway</servlet-name>
		<url-pattern>/TOUPPER-HttpInbound</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>15</session-timeout>
	</session-config>
</web-app>
